.s-results .inner-results .btn:hover, .s-results .inner-results .btn:focus { box-shadow: none; background: inherit; }

.s-results .inner-results .btn-u-default { color: rgb(153, 153, 153); border: none; }

.s-results .inner-results .btn-u-xs { padding: 0px; }

.s-results .inner-results .btn-u-default i { font-size: 14px; }

.s-results .inner-results p { margin-bottom: 5px; }

.s-results .down-ul { margin-bottom: 0px; }

.s-results .down-ul li { color: rgb(153, 153, 153); }

.s-results .down-ul li a { color: rgb(114, 192, 44); }

.s-results .star-vote { margin-left: 0px; }

.s-results .star-vote li { padding: 0px; }

@media (max-width: 992px) {
  .s-results .related-search { border-right: none; margin-bottom: 30px; padding-bottom: 10px; border-bottom: 1px solid rgb(238, 238, 238); }
  .s-results .related-search hr { margin: 10px 0px; visibility: hidden; }
}

@media (max-width: 500px) {
  .s-results .inner-results img { width: 60px; height: auto; }
  .s-results .inner-results .overflow-a p { display: none; }
}
